/*npm init
npm install express -S -E
cat package.json*/

// Imports
const express = require('express')
const app = express()
// Puerto
const PORT = 3000
// Middlewares
app.use(express.json())
// Arrancar Servidor
app.listen(PORT, () => {
    console.log(`Server is running. http://localhost:${PORT}`)
})

// fuente: 'https://www.alexcantongarcia.es/blog/como-crear-un-servidor-web-en-javascript-con-express'